{
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": ["error", {
        "singleQuote": true,
        "trailingComma": "es5",
        "endOfLine": "auto"
      }],
      "max-len": ["error", { "code": 80 }],
      "indent": ["error", 2],
      "no-multi-spaces": "error"
    }
  }
  